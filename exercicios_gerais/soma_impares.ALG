Algoritmo "soma_impares"

Var

   x, y, troca, soma, i : inteiro

Inicio

   soma <- 0
   
   escreval("Digite dois números: ")
   leia(x)
   leia(y)
   
   se x > y entao
     troca <- x
     x <- y
     y <- troca
   fimse
   
   para i de (x + 1) ate (y - 1) faca
      se i % 2 <> 0 entao
         soma <- soma + i
      fimse
   fimpara

   escreva("SOMA DOS ÍMPARES = ", soma)

Fimalgoritmo